{
  "name": "@glyphide/quickjs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "precompile quickjs utils with polyfills for glyphide.",
  "author": "Pedro Carre√±o (34664891+Pkcarreno@users.noreply.github.com)",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.umd.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "type-check": "tsc --project tsconfig.json --noEmit"
  },
  "keywords": [
    "glyphide",
    "sebastianwessel/quickjs",
    "polyfills",
    "vite"
  ],
  "license": "MIT",
  "dependencies": {
    "@sebastianwessel/quickjs": "^2.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkcarreno/glyphide.git"
  }
}
