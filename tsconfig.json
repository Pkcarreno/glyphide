{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "types": ["vite-plugin-pwa/react", "vite-plugin-pwa/info"]
  },
  "exclude": ["node_modules", "dist"],
  "include": [".astro/types.d.ts", "**/*", "src"]
}
